{
  "name": "tenant-coordinator",
  "version": "0.1.0",
  "description": "Tool for coordinating efforts, reimbursements, plans and rent payments with my tenant",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "coffee -b -c -M -o dist/static/js static/coffee",
    "check-env": "scripts/check-env",
    "clean": "rm -rf dist/ tenant-coordinator.db* node_modules/.cache/ && echo 'Clean complete'",
    "clean:all": "npm run clean && rm -rf node_modules/ && echo 'Full clean complete - run npm install'",
    "dev": "npm run build && coffee main.coffee",
    "start": "coffee main.coffee",
    "test": "coffee test/services/rent.coffee",
    "test:focus": "coffee scripts/run-tests.coffee",
    "test:integration": "coffee scripts/test-integration.coffee",
    "lint": "echo 'Linting skipped - configure if needed'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "coffeescript": "^2.7.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "nodemailer": "^6.9.0",
    "stripe": "^17.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "chokidar": "^3.6.0"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "author": "Robert de Forest <robert@defore.st>",
  "license": "MIT"
}
